{% extends "layout.html.twig" %}

{% block body_main %}
    {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
        {{ form_errors(form) }}

        <div class="form-group">
            {{ form_row(form.name) }}
        </div>
        <div class="form-group">
            {{ form_row(form.username) }}
        </div>
        <div class="form-group">
            {{ form_row(form.password) }}
        </div>
        <div class="form-group">
            {{ form_row(form.url) }}
        </div>
        <div class="form-group">
            {{ form_row(form.notes) }}
        </div>
        <div class="form-group">
            {{ form_row(form.save) }}
        </div>
    {{ form_end(form) }}
{% endblock body_main %}
